<!-- default new form --> 
<h1> Login </h1>
<%=form_tag user_sessions_path do %>
  <div class="field">
    <%= label_tag :username %><br />
    <%= text_field_tag :username %>
  </div>
 
 <div class="field">
    <%= label_tag :password%><br />
    <%= text_field_tag :password%>
  </div>

 <%= submit_tag 'submit' %>
<% end %>
